{
  "name": "github-repo-analyzer",
  "framework": "create-react-app",
  "files": [
    "package.json",
    "package-lock.json",
    "public/**",
    "src/**",
    "node_modules/**",
    "netlify.toml",
    "windsurf.json"
  ],
  "build": {
    "command": "npm ci && npm run build",
    "output": "build"
  },
  "routes": {
    "/": "index.html"
  }
}
