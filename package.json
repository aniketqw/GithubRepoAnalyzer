{
  "name": "github-repo-analyzer",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.6",
    "@mui/material": "^5.15.6",
    "axios": "^1.9.0",
    "chart.js": "^4.4.1",
    "concurrently": "^9.1.2",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.2",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "cd server && npm start",
    "server-dev": "cd server && npm run dev",
    "dev": "concurrently \"npm run server-dev\" \"npm start\"",
    "install-all": "npm install && cd server && npm install",
    "check-env": "cd server && npm run check-env",
    "docker-build": "docker build -t github-repo-analyzer .",
    "docker-run": "docker run -p 3000:3000 -p 3001:3001 github-repo-analyzer"
  },
  "proxy": "http://localhost:3001",
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
